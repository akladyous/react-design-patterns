{
  "trailingComma": "all",
  "useTabs": false,
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "bracketSameLine": true,
  "overrides": [],
  "printWidth": 80,
  "singleAttributePerLine": true,
  "rules": {},
  "react/jsx-max-props-per-line": [1, { "when": "always" }],

  "plugins": ["react", "prettier"],

  "extends": ["eslint:recommended", "plugin:react/recommended"]
}
